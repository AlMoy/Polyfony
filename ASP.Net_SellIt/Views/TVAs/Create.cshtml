@model EntityASP.Entity.TVA

@{
    ViewBag.Title = "Création des taux TVA";
}
@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        <div class="input-group mt-auto">
            <div class="input-group-prepend">
                @Html.LabelFor(m => m.EndDate, new { @class = "input-group-text" })
            </div>
            @Html.EditorFor(m => m.EndDate, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.EndDate, "", new { @class = "text-danger" })
        </div>
        <div class="input-group mt-3">
            <div class="input-group-prepend">
                @Html.LabelFor(m => m.Rate, new { @class = "input-group-text" })
            </div>
            @Html.EditorFor(m => m.Rate, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.Rate, "", new { @class = "text-danger" })
        </div>
                
            <div class="col-md-offset-2 mt-3">
                @Html.ActionLink("Retour à la liste", "Index", "TVAs", null, htmlAttributes: new { @class = "btn btn-primary" })
                <input type="submit" value="Créér" class="btn btn-primary" />
            </div>              

    </div>

}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
