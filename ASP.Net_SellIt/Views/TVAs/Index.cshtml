@model IEnumerable<EntityASP.Entity.TVA>

@{
    ViewBag.Title = "Liste des taux TVA";
}


<div class="d-flex flex-column">
    <div class="mb-2">
        @Html.ActionLink("Retour", "Divers", "Users", null, new { @class = "btn btn-secondary text-secondary" })

        @Html.ActionLink("Nouveau", "Create", "TVAs", new { @class = "btn btn-primary text-primary" })
    </div>
    <table class="table table-striped table-bordered table-hover">
        <thead class="thead-dark">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EndDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Rate)
                </th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.EndDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Rate)
                    </td>
                    <td>
                        @Html.ActionLink("Editer", "Edit", new { id = item.Id })
                        @Html.ActionLink("Détails", "Details", new { id = item.Id })
                        @Html.ActionLink("Supprimer", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }
        </tbody>

    </table>
</div>
